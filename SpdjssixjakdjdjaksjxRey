
--// Fish World Script Hub (Linoria UI)
--// By Rey | Auto Save + Auto Login

-- Destroy old UI
pcall(function()
    game:GetService("CoreGui"):FindFirstChild("Linoria") and game:GetService("CoreGui"):FindFirstChild("Linoria"):Destroy()
end)

-- Load Linoria
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/violin-suzutsuki/LinoriaLib/main/Library.lua"))()
local ThemeManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/violin-suzutsuki/LinoriaLib/main/addons/ThemeManager.lua"))()
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/violin-suzutsuki/LinoriaLib/main/addons/SaveManager.lua"))()

-- CONFIG
local CORRECT_KEY = "ReyScriptHubFishWorldSoOp"
local DISCORD = "https://discord.gg/G4n8yzBx2J"

local isAuthenticated = false
local savedKey = ""
local speed1, speed2 = 1, 1
local logs = {}

-- Utils
local function cleanKey(v)
    return tostring(v or ""):gsub("%s+", "")
end

local function addLog(txt)
    table.insert(logs, os.date("[%H:%M:%S] ") .. txt)
    if #logs > 20 then table.remove(logs, 1) end
end

-- Window
local Window = Library:CreateWindow({
    Title = "ðŸŽ£ Fish World Hub",
    Center = true,
    AutoShow = true,
})

-- Tabs
local Tabs = {
    Key = Window:AddTab("Key System"),
    Main = Window:AddTab("Main"),
    Settings = Window:AddTab("Settings"),
    Console = Window:AddTab("Console"),
    Info = Window:AddTab("Info")
}

-- Save Manager
SaveManager:SetLibrary(Library)
SaveManager:SetFolder("FishWorldHub")
SaveManager:BuildConfigSection(Tabs.Settings)

ThemeManager:SetLibrary(Library)
ThemeManager:ApplyToTab(Tabs.Settings)

-- KEY INPUT
Tabs.Key:AddInput("KeyInput", {
    Title = "Enter Key",
    Placeholder = "Paste key here...",
    Callback = function(v)
        savedKey = cleanKey(v)
        SaveManager:Save()
    end
})

Tabs.Key:AddButton("Verify Key", function()
    if cleanKey(savedKey) == cleanKey(CORRECT_KEY) then
        isAuthenticated = true
        SaveManager:Save()
        Library:Notify("Success", "Key verified!", 3)
        addLog("Key verified")
    else
        Library:Notify("Error", "Invalid key!", 3)
    end
end)

-- AUTO LOGIN
task.delay(1, function()
    if cleanKey(savedKey) == cleanKey(CORRECT_KEY) then
        isAuthenticated = true
        Library:Notify("Auto Login", "Key loaded successfully", 3)
    end
end)

-- SCRIPT FUNCTIONS
local function Script1()
    task.wait(speed1)
    game:GetService("ReplicatedStorage").communication["9362e25a8d224fd987d0d3f50d8249bc"]:FireServer({
        Vector3.new(-269.9, 15.8, -262.7),
        Vector3.new(-0.43, 0, -0.9),
        1.95,
        false
    })
    addLog("Script 1 executed")
end

local function Script2()
    task.wait(speed2)
    game:GetService("ReplicatedStorage").communication["fc57ae91538d430fa1ff9a33be3bc18c"]:FireServer({
        true,
        Vector3.new(-274.2, 3.4, -274.8)
    })
    addLog("Script 2 executed")
end

-- MAIN
Tabs.Main:AddButton("â–¶ Execute All", function()
    if not isAuthenticated then return end
    task.spawn(function()
        Script1()
        Script2()
    end)
end)

Tabs.Main:AddButton("âš¡ Script 1", function()
    if isAuthenticated then task.spawn(Script1) end
end)

Tabs.Main:AddButton("âš¡ Script 2", function()
    if isAuthenticated then task.spawn(Script2) end
end)

-- SETTINGS
Tabs.Settings:AddSlider("Delay1", {
    Text = "Script 1 Delay",
    Min = 0,
    Max = 10,
    Default = 1,
    Callback = function(v) speed1 = v end
})

Tabs.Settings:AddSlider("Delay2", {
    Text = "Script 2 Delay",
    Min = 0,
    Max = 10,
    Default = 1,
    Callback = function(v) speed2 = v end
})

-- CONSOLE
local consoleLabel = Tabs.Console:AddLabel("Waiting...")

task.spawn(function()
    while true do
        consoleLabel:SetText(table.concat(logs, "\n"))
        task.wait(0.3)
    end
end)

-- INFO
Tabs.Info:AddParagraph("Developer", "Rey")
Tabs.Info:AddParagraph("Discord", DISCORD)

Library:Notify("Loaded", "Fish World Script Ready!", 4)

print("âœ… Fish World Script Loaded (Linoria UI)")
