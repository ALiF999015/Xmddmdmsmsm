    addLog("‚úÖ Discord link disalin!")
end)

local keyBox = createTextBox(keyTab.content, "Masukkan key dari Discord...")

createButton(keyTab.content, "‚úÖ Verify Key", "Klik untuk verifikasi", function()
    if keyBox.Text == CORRECT_KEY then
        isAuthenticated = true
        addLog("üéâ Key valid! Akses diberikan")
    else
        addLog("‚ùå Key salah!")
    end
end)

createLabel(keyTab.content, "üí° Cara Mendapatkan Key", "1. Join Discord\n2. Cari #get-key\n3. Follow instruksi")

-- Main Tab Content
createLabel(mainTab.content, "üé£ Script Executor", "Jalankan script dengan delay custom")

local statusLabel = createLabel(mainTab.content, "üìä Status:", "‚è≥ Siap dijalankan")

createButton(mainTab.content, "‚ñ∂Ô∏è Execute All Scripts", "Jalankan Script 1 ‚Üí Script 2", function()
    executeBothScripts()
end)

createButton(mainTab.content, "‚ö° Execute Script 1", "Jalankan Script 1 saja", function()
    if not isAuthenticated then
        addLog("üîí Verifikasi key terlebih dahulu!")
        return
    end
    
    if not isExecuting then
        isExecuting = true
        task.spawn(function()
            pcall(executeScript1)
            isExecuting = false
        end)
    end
end)

createButton(mainTab.content, "‚ö° Execute Script 2", "Jalankan Script 2 saja", function()
    if not isAuthenticated then
        addLog("üîí Verifikasi key terlebih dahulu!")
        return
    end
    
    if not isExecuting then
        isExecuting = true
        task.spawn(function()
            pcall(executeScript2)
            isExecuting = false
        end)
    end
end)

-- Settings Tab Content
createLabel(settingsTab.content, "‚öôÔ∏è Pengaturan Delay", "Atur delay eksekusi dalam detik")

local speed1Box = createTextBox(settingsTab.content, "Delay Script 1 (detik) - Default: 1")
speed1Box.Text = "1"
speed1Box.FocusLost:Connect(function()
    local value = tonumber(speed1Box.Text)
    if value and value >= 0 then
        speed1 = value
        addLog("‚öôÔ∏è Delay Script 1 diatur: " .. value .. "s")
    else
        speed1Box.Text = tostring(speed1)
        addLog("‚ö†Ô∏è Input tidak valid! Gunakan angka >= 0")
    end
end)

local speed2Box = createTextBox(settingsTab.content, "Delay Script 2 (detik) - Default: 1")
speed2Box.Text = "1"
speed2Box.FocusLost:Connect(function()
    local value = tonumber(speed2Box.Text)
    if value and value >= 0 then
        speed2 = value
        addLog("‚öôÔ∏è Delay Script 2 diatur: " .. value .. "s")
    else
        speed2Box.Text = tostring(speed2)
        addLog("‚ö†Ô∏è Input tidak valid! Gunakan angka >= 0")
    end
end)

createLabel(settingsTab.content, "üìä Atau Gunakan Slider", "Slider untuk pengaturan cepat")

createSlider(settingsTab.content, "‚ö° Delay Script 1 (Slider)", 0, 10, 1, function(value)
    speed1 = value
    speed1Box.Text = tostring(value)
    addLog("‚öôÔ∏è Delay Script 1: " .. value .. "s")
end)

createSlider(settingsTab.content, "‚ö° Delay Script 2 (Slider)", 0, 10, 1, function(value)
    speed2 = value
    speed2Box.Text = tostring(value)
    addLog("‚öôÔ∏è Delay Script 2: " .. value .. "s")
end)

