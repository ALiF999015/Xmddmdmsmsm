    local v = tonumber(speed1Box.Text)
    if v and v >= 0 then
        speed1 = v
        addLog("âš™ï¸ Delay 1: " .. v .. "s")
    else
        speed1Box.Text = tostring(speed1)
        addLog("âš ï¸ Input invalid!")
    end
end)

